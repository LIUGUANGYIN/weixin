<template name="starTemplate">
  <view class="start-box">
    <block wx:for="{{stars}}" wx:for-item="num" wx:key="">
      <image wx:if="{{num == 1}}" src='/images/swiper/x1.jpg'></image>
      <image wx:else src='/images/swiper/x1.jpg'></image>
    </block>
    <text class="score">5</text>
  </view>
</template> 